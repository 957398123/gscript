// 王辅命令hook
function chatCommandHook(channel, id, name, msg){
    var cmd = msg.substring(0,3);
    if(cmd == "000"){  // 在线统计
        // 回复hello
        gameEnv.sendCommandMessage(1, id, "001");
    }else if(cmd == "001"){
        printMessage("[王辅]：王辅会员" + name + "向您报道！");
    }else if(cmd == "002"){  // 设置优先攻击目标
        // 如果自己当前的优先攻击对象为0，才进行设置
        if(player.priorityAttack == 0){
            // 获取id编号
            var id = msg.substring(3, -1);
            // 更新优先攻击目标
            player.setPriorityAttack(id);
        }
    }
}